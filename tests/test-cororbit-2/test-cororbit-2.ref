// assign alignment errors in ring 1 and ring 2 at Q1 

Use, period=lhcb2;

Select, flag=ERROR, clear;

Select, flag=ERROR, range=MQXA.1L1;

ealign, dy:=0.100e-3, dx:=0.070e-3;

Select, flag=ERROR, clear;

Select, flag=ERROR, range=MQXA.1R5;

ealign, dy:=0.050e-3, dx:=0.035e-3;



Use, period=lhcb1;

Select, flag=ERROR, clear;

Select, flag=ERROR, range=MQXA.1L1;

ealign, dy:=0.100e-3, dx:=0.070e-3;

Select, flag=ERROR, clear;

Select, flag=ERROR, range=MQXA.1R5;

ealign, dy:=0.050e-3, dx:=0.035e-3;



