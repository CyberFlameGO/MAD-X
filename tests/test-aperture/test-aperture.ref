title,'LHC Cell APERTURE example';



REAL CONST ANGLE.MB = TWOPI/8/(23*6+2*2*4);



REAL CONST l.MB = 14.30;

REAL CONST l.MQ = 3.10;



REAL CONST drift1 = 3.67;

REAL CONST drift2 = 1.02;

REAL CONST drift3 = 1.74;



len_half_cell = l.MQ+drift1+3*l.MB+2*drift2+drift3;

Value, len_half_cell;

len_half_cell =              53.45 ;


!!!! apertures

a=0.022; b=0.018; h=0.022; v=0.022; an1=pi/6; an2=pi/3;

!!!! tolerances

th=0.001; tv=0.0005; tr=0.001;



offset=1.e-3;

off3 := 1.e-3*l.MQ*l.MQ;

off2 := 2.e-3 * l.MQ;

off1 = 3.e-3;



MB: SBEND, L=l.MB, angle=ANGLE.MB;

MQ: QUADRUPOLE, L=l.MQ, apertype=rectellipse,aperture = {a,b,h,v}, aper_tol = {th,tv,tr}

                aper_offset={off1, off2, off3, offset*l.MQ, 0.005-l.MQ, 0.006+l.MQ};



kqf =  0.008703550815;

kqd = -0.008705002933;



LHCCELL: SEQUENCE, refer = ENTRY, L = 2*len_half_cell; 

  MQ1: MQ,  at= 0., k1=kqf;

  MB1: MB,  at= l.MQ+drift1 ,

	    apertype=circle, aperture = {a}, aper_tol = {th,tv,tr};

  MB2: MB,  at= l.MQ+drift1+l.mb+drift2,

	    apertype=ellipse, aperture = {a,b}, aper_tol = {th,tv,tr};

  MB3: MB,  at= l.MQ+drift1+2*(l.mb+drift2),

	    apertype=rectangle, aperture = {a,b}, aper_tol = {th,tv,tr};

  MQ2: MQ,  at=  len_half_cell, k1 := kqd,

            apertype=octagon, aperture={a,b,an1,an2}, aper_tol = {th,tv,tr};

  MB4: MB,  at= len_half_cell + l.MQ+drift1 ,

	    apertype=rectcircle, aperture = {a,b,h}, aper_tol = {th,tv,tr};

  MB5: MB,  at= len_half_cell + l.MQ+drift1+l.mb+drift2,

	    apertype=lhcscreen, aperture = {a,b,h}, aper_tol = {th,tv,tr};

  MB6: MB,  at= len_half_cell + l.MQ+drift1+2*(l.mb+drift2),

	    apertype=racetrack, aperture = {a+h/5.,b+h/10.,h/5.,h/10.}, aper_tol = {th,tv,tr};

endsequence;



beam,particle=proton,energy=450.0, exn=3.75e-6, eyn=3.75e-6;



