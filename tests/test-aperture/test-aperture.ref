LHCCEL2: SEQUENCE, refer = ENTRY, L = 2*len_half_cell; 

  MQ11: MQ, k1= kqf, at= 0., aperture = {c,d,h2,v2},

           aper_offset={off1, off2, off3, offset*l.MQ, 0.005-l.MQ, 0.0};

  MB11: MB,  at= l.MQ+drift1 ,

	    apertype=circle, aperture = {c}, aper_tol = {th,tv,tr};

  MB21: MB,  at= l.MQ+drift1+l.mb+drift2,

	    apertype=ellipse, aperture = {c,d}, aper_tol = {th,tv,tr};

  MB31: MB,  at= l.MQ+drift1+2*(l.mb+drift2),

	    apertype=rectangle, aperture = {c,d}, aper_tol = {th,tv,tr};

  MQ21: MQ, k1= kqd,  at=  len_half_cell, 

            apertype=octagon, aperture = {c,d,an2,an1}, aper_tol = {th,tv,tr};

  MB41: MB,  at= len_half_cell + l.MQ+drift1 ,

	    apertype=rectcircle, aperture = {c,d,h2}, aper_tol = {th,tv,tr};

  MB51: MB,  at= len_half_cell + l.MQ+drift1+l.mb+drift2,

	    apertype=lhcscreen, aperture = {c,d,h2}, aper_tol = {th,tv,tr};

  MB61: MB,  at= len_half_cell + l.MQ+drift1+2*(l.mb+drift2),

	    apertype=racetrack, aperture = {c+h2/5.,d+h2/10.,h2/5.,h2/10.}, aper_tol = {th,tv,tr};

endsequence;



