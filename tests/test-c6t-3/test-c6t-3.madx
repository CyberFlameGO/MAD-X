TITLE;
option,-echo,-warn;
beam;

kqf = .872651312e-2;
mq: quadrupole, l=3.1, apertype=ellipse,aperture={0.02,0.01};

fivecell: sequence, refer=centre, l=10;
   seqstart: marker, at = 0;
   qf.1: mq, k1:=kqf, at=5, from=seqstart;
end_fivecell: marker at=10;
endsequence;

use,period=fivecell;   ! use needed for twiss (not necessarily needed before makethin)
twiss;

sixtrack, aperture;
removefile file="fc.3"
stop;
