TITLE;
option,-echo,-warn;
beam;

kqf =  .872651312e-2;
kqd = -.872651312e-2;
mq: quadrupole, l=3, apertype=ellipse,aperture={0.02,0.01};

fivecell: sequence, refer=centre, l=10;
   qf.1: mq, k1:=kqf, at=3;
   qd.1: mq, k1:=kqd, at=7;
end_fivecell: marker at=10;
endsequence;

use,period=fivecell;   ! use needed for twiss (not necessarily needed before makethin)
twiss;

sixtrack;
removefile file="fc.3"
stop;
