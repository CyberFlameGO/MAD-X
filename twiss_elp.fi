      integer    b_angle , b_tilt , b_k0 , b_k0s ,                      &
     &           b_k1 , b_k1s , b_e1 , b_e2 , b_k2 ,                    &
     &           b_k2s , b_h1 , b_h2 , b_hgap ,                         &
     &           b_fint , b_fintx , b_k3 , b_k3s
      parameter (b_angle = 7, b_tilt = 8, b_k0 = 9, b_k0s = 10,         &
     &           b_k1 = 11, b_k1s = 12, b_e1 = 13 , b_e2 = 14, b_k2 =15,&
