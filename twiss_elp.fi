      parameter (so_lrad = 7, so_ks = 8, so_ksi = 9)
!-rfc------------------------------------------------------------------*
      integer    r_volt, r_lag , r_freq
      parameter (r_volt = 7, r_lag = 8, r_freq = 9)
!-elsep----------------------------------------------------------------*
