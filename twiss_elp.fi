      parameter (h_tilt = 7, h_lrad = 8, h_kick = 9, h_hkick = 10,       &
     &           h_chkick = 11)
!-hkick----------------------------------------------------------------*
      integer    v_tilt, v_lrad , v_kick, v_vkick, v_cvkick
      parameter (v_tilt = 7, v_lrad = 8, v_kick = 9, v_vkick = 10,       &
     &           v_cvkick = 11)
!-kick-----------------------------------------------------------------*
      integer    k_tilt, k_lrad , k_hkick, k_vkick, k_chkick,            &
     &           k_cvkick
