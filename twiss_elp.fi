!---fixed positions for element parameters-----------------------------*
      common / el_pos_par / g_elpar(50)
      double precision g_elpar
!-general--------------------------------------------------------------*
      integer    g_el, g_kmax, g_kmin, g_calib, g_polarity
      parameter (g_el = 2, g_kmax = 3, g_kmin = 4, g_calib = 5,         &
     &           g_polarity = 6)
