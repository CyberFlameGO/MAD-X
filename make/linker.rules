.PHONY: $(PROJECT)

$(PROJECT): $(OBJECTS)
	$E "*** Linking $(@F)"
	$_ $(LD) $(LDFLAGS) -o $@ $^ $(LIBS) $(LDLIBS)

# end of makefile
